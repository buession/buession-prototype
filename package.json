    "remark-stringify": "^11.0.0",
    "remark-yaml-config": "^7.0.0",
    "rollup": "^4.0.2",
    "through2": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "unified": "^9.2.2"
